<?xml version="1.0" encoding="utf-8"?>
<log4net>
  <appender name="console" type="log4net.Appender.ConsoleAppender">
    <threshold value="ALL" />
    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%-5p %d [%t] %c: %m%n" />
    </layout>
  </appender>
  <root>
    <level value="ALL" />
    <appender-ref ref="console" />
  </root>
</log4net>
